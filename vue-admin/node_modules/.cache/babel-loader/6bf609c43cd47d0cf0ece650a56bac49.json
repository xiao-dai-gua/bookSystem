tton>取消</el-button>\r\n  </el-form-item>\r\n</el-form>\r\n </div>\r\n        </div>\r\n</template>\r\n<script>\r\n  //函数名叫什么参数定义就是什么\r\n  import {getMessage} from '../../api/message.js'//或者@/api/message表示src下的message\r\n  import {addLevel} from '../../api/level.js'\r\n  export default {\r\n    created() {\r\n      this.getData()\r\n      this.addData()\r\n    },\r\n    methods: {\r\n      onSubmit() {\r\n        console.log('submit!');\r\n      },\r\n      //添加数据\r\n       getData(){\r\n        getMessage().then((res)=>{\r\n          console.log(res);\r\n        })\r\n      },\r\n      //获取数据\r\n      addData(){\r\n       addLevel({\r\n          levelname:\"testa\"\r\n        }).then((res)=>{\r\n          console.log(res);\r\n        })\r\n      }\r\n    },\r\n    data() {\r\n      return {\r\n        form: {\r\n          name: '',\r\n          region: '',\r\n          date1: '',\r\n          date2: '',\r\n          delivery: false,\r\n          type: [],\r\n          resource: '',\r\n          desc: ''\r\n        }\r\n      }\r\n    },\r\n  }\r\n</script>\r\n<style  lang=\"scss\"sscoped>//使用scss一定要声明一下\r\n$w:800px;\r\n    .form{\r\n        width:$w;\r\n    }\r\n</style>"],"sourceRoot":"src/views/test_page"}]}